source 'https://rubygems.org'
ruby '3.2.3' # Asegúrate de tener la versión correcta de Ruby

# Usa la gema github-pages solamente
gem 'github-pages', group: :jekyll_plugins, source: 'https://rubygems.org'

# Incluye cualquier otra gema que necesites para tu sitio Jekyll
# Por ejemplo, si estás utilizando el tema Minima:
gem 'minima', '~> 2.5'

# Si estás utilizando plugins de Jekyll, asegúrate de incluirlos en el grupo :jekyll_plugins
group :jekyll_plugins do
  gem 'jekyll-feed', '~> 0.12'
end